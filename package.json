{
  "name": "anki-obsidian-plugin",
  "private": true,
  "version": "0.5.1",
  "description": "A powerful spaced repetition learning plugin for Obsidian, featuring FSRS algorithm and modern UI",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "vite build --mode development --watch",
    "build": "vite build --mode production",
    "postbuild": "node scripts/copy-manifest.cjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "biome format --write ./src",
    "lint": "biome lint --apply-unsafe ./src",
    "clean": "rimraf dist node_modules/.vite",
    "reinstall": "npm run clean && npm install"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "anki",
    "spaced-repetition",
    "fsrs",
    "learning",
    "flashcards",
    "svelte",
    "svelte5",
    "typescript"
  ],
  "author": "Tuanki Team",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.3.3",
    "@codemirror/autocomplete": "6.18.6",
    "@codemirror/commands": "6.8.1",
    "@codemirror/lang-markdown": "6.3.4",
    "@codemirror/language": "6.11.2",
    "@codemirror/state": "6.5.0",
    "@codemirror/view": "6.38.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/svelte": "5.2.8",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^20.8.10",
    "@types/sql.js": "1.4.9",
    "@unocss/extractor-svelte": "^0.57.2",
    "builtin-modules": "^3.3.0",
    "bun-types": "latest",
    "dotenv": "17.2.1",
    "jsdom": "26.1.0",
    "obsidian": "^1.4.11",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "terser": "5.43.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unocss": "^0.57.2",
    "vite": "^5.0.0",
    "vite-plugin-commonjs": "0.10.4",
    "vite-plugin-css-injected-by-js": "^3.5.0",
    "vite-plugin-static-copy": "^3.1.1",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "@codemirror/lang-css": "6.3.1",
    "@codemirror/lang-html": "6.4.9",
    "@codemirror/lang-javascript": "6.2.4",
    "@codemirror/lang-json": "6.0.2",
    "@codemirror/lang-python": "6.2.1",
    "@codemirror/lang-sql": "6.9.1",
    "@codemirror/lang-xml": "6.1.0",
    "@codemirror/search": "6.5.11",
    "@codemirror/theme-one-dark": "6.1.3",
    "@floating-ui/dom": "1.7.4",
    "@replit/codemirror-vim": "6.3.0",
    "@tanstack/svelte-virtual": "3.13.12",
    "echarts": "6.0.0",
    "jszip": "3.10.1",
    "pako": "^2.1.0",
    "sql.js": "1.13.0"
  }
}
